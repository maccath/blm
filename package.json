{
  "name": "maccath-blm",
  "private": true,
  "description": "Black Lives Matter - A Compilation of UK-centric resources",
  "version": "0.1.0",
  "author": "Katy Ereira <katy.ereira@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-plugin-styled-components": "^3.3.3",
    "gatsby-plugin-typography": "^2.5.3",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "styled-components": "^5.1.1",
    "typography": "^0.16.19",
    "typography-theme-kirkham": "^0.16.19"
  },
  "devDependencies": {
    "commitizen": "^4.1.2",
    "emoji-cz": "^0.3.1",
    "husky": "^4.2.5",
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby",
    "blm"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maccath/blm"
  },
  "bugs": {
    "url": "https://github.com/maccath/blm/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/emoji-cz",
      "format": "[emoji] [name]: [subject]",
      "emoji-cz": {
        "types": {
          "Configuration": {
            "emoji": "ðŸ”§",
            "description": "Changing configuration files."
          },
          "Text": {
            "emoji": "ðŸ’¬",
            "description": "Updating text and literals."
          }
        }
      }
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
